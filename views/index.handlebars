<h1>Eat-Da Burger</h1>
<img src="https://banner2.cleanpng.com/20190115/ubv/kisspng-hamburger-street-food-fizzy-drinks-computer-icons-cepat-makanan-burger-ikon-gratis-dari-fast-food-5c3e373c9d0621.9963069215475812446432.jpg"
    alt="picture of burger" />


<div class="row center-align">
    <div class="col s12 m6">
        <div class="row center-align">
            <h3>All Burgers</h3>
        </div>
        <div class="row">
            <ul>
                {{#each burgers}}
                {{/each}}
            </ul>
        </div>

    </div>

    <div class="col s12 m6">
        <div class="row center-align">
            <h3>Devoured Burgers</h3>
        </div>
        <div class="row">
            <ul>
                {{#each burgers}}
                {{/each}}
            </ul>
        </div>
    </div>
</div>

<div class="row center-align">
    <form class="col s12">
        <div class="row">
            <div class="input-field col s6">
                <i class="material-icons prefix">local_dining</i>
                <textarea id="burger" class="materialize-textarea"></textarea>
                <label for="burger">Burger Name</label>
            </div>
            <a class="waves-effect waves-light btn"><i class="material-icons left">check</i>button</a>
        </div>
    </form>
</div>